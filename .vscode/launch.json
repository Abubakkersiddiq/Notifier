            "program": "${workspaceFolder}/server.js",
            "env": {
                "REDIS_AUTH_URL": "redis://localhost:6379",
                "MONGO_DATABASE_URI" :"mongodb://localhost:27017",
                "REDIS_STORE_URI" : "redis://localhost:6379",
                "REDIS_STORE_SECRET": "cat"
            }
        }
    ]
}